	<jxb:bindings schemaLocation="xhtml5.xsd" node="/xs:schema">
		<jxb:bindings node="xs:element[@name='select']">
			<jxb:class name="SelectElement" />
		</jxb:bindings>
		<jxb:bindings node="xs:element[@name='p']">
